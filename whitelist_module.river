  argument "targets" {
      optional = true
    }

discovery.kubernetes "k8s_pods" {
  role = "pod"

  selectors {
    role = "pod"
    field = "metadata.namespace!=default"
  }

  namespaces {
    names = ["grafana"]
  }
}

    export "allowList" {
      value =   json_decode(discovery.kubernetes.k8s_pods.targets)
    }
