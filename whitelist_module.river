    argument "cluster" {
      optional = false
    }


    remote.http "cluster_config" {
      url = "https://raw.githubusercontent.com/marioanton/test_module_git/main/" + argument.cluster.value[0] + "/allowlist.json"
    }


export "allowList" {
  value =   json_decode(remote.http.cluster_config.content).allowList
}
